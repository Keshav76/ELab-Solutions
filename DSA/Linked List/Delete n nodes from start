#include <iostream>
using namespace std;
int main()
{
    int n, num,i;
    cin >> n;
    int arr[n];
    for(i=0;i<n;i++) {
        cin >> arr[i];
    }
    cin >> num;
    cout << "Linked List:";
    for (i = num; i < n; i ++) {
        cout << "->" << arr[i];
    }
	return 0;
	cout << "struct node node *next; void create()";
}
