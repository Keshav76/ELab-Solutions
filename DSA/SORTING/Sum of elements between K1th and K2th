#include <bits/stdc++.h>
using namespace std;
int main()
{
    int t; cin >> t;
    while(t>0) {
        int n, f,s,i,count=0;
        cin >> n;
        int arr[n];
        for(i=0;i<n;i++) {
            cin >> arr[i];
        }
        cin >> f >> s;
        sort(arr,arr+n);
        for(i=f;i<s-1;i++)count+=arr[i];
        cout<<count<<endl;
        t--;
    }
	return 0;
}
